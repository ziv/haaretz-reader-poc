{
  "name": "Haaretz/Themarker Reader",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Read Haaretz/Themarker premium articles without login",
  "permissions": [
    "tabs",
    "webRequest",
    "background",
    "webRequestBlocking",
    "https://*.haaretz.co.il/*",
    "https://*.themarker.com/*"
  ],
  "background": {
    "scripts": [
      "main.js"
    ]
  }
}
